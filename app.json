{
  "expo": {
    "name": "Bitolj Popis",
    "slug": "bitolj_popis",
    "version": "1.1.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "bitoljapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lazarpav.bitolj_popis"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/android_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8cc7cbb0-c70b-4f83-85ae-b0bdd68023b2"
      }
    }
  }
}
